  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue,cjs}": [
      "eslint --fix"
    ],
    "*.{css,vue}": "stylelint --fix"
  },
  "ct3nMetaData": {
    "initVersion": "0.1.1"
  }
