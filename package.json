{
  "name": "predictor-n3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && prisma generate && pnpm simple-git-hooks install",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:js": "eslint --ext .js,.ts,.mjs,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "pisma-db-push": "npx prisma db push",
    "preview": "nuxt preview",
    "prisma-generate": "npx prisma generate",
    "test": "vitest --run",
    "test:types": "nuxt typecheck",
    "test:ui": "vitest --ui"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint --fix"
    ],
    "*.{css,vue}": "stylelint --fix"
  },
