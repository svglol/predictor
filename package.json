{
  "name": "predictor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:vercel": "NITRO_PRESET='vercel' nuxt build",
    "build:vercel-edge": "NITRO_PRESET='vercel-edge' nuxt build",
    "db:push": "drizzle-kit push:mysql",
    "dev": "nuxt dev",
    "drizzle-studio": "drizzle-kit studio",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install && pnpm dlx playwright-core install",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:js": "eslint --ext .js,.ts,.mjs,.vue,cjs --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "test:types": "nuxt typecheck",
    "test:ui": "vitest --ui"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue,cjs}": [
      "eslint --fix"
    ],
    "*.{css,vue}": "stylelint --fix"
  },
