    "@tiptap/vue-3": "2.2.4",
    "@trpc/client": "10.45.2",
    "@trpc/server": "10.45.2",
    "@types/node": "20.12.7",
    "@types/pluralize": "0.0.33",
    "@types/streamifier": "0.1.2",
    "@vite-pwa/nuxt": "0.6.0",
    "@vitest/ui": "1.4.0",
    "@vue-macros/nuxt": "1.9.30",
