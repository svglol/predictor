    "@tiptap/extension-table-row": "2.11.5",
    "@tiptap/extension-text-align": "2.11.5",
    "@tiptap/pm": "2.11.5",
    "@tiptap/starter-kit": "2.11.5",
    "@tiptap/vue-3": "2.11.5",
    "@vite-pwa/nuxt": "0.10.8",
    "@vue-macros/nuxt": "1.13.5",
    "@vueuse/nuxt": "12.8.2",
    "apexcharts": "4.5.0",
    "cloudinary": "2.6.0",
    "drizzle-kit": "0.30.5",
    "drizzle-orm": "0.41.0",
    "maska": "2.1.11",
    "nuxt-time": "1.0.3",
    "pluralize": "8.0.0",
    "slugify": "1.6.6",
    "superjson": "2.2.2",
    "tailwindcss": "3.4.17",
    "trpc-nuxt": "0.11.1",
    "v-calendar": "3.1.2",
    "vue-slicksort": "2.0.5",
    "vue3-apexcharts": "1.8.0",
    "zod": "3.24.2"
